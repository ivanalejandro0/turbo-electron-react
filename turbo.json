{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "desktop#start": {
      "dependsOn": ["desktop#build"],
      "cache": false
    },
    "desktop#bundle": {
      "dependsOn": ["desktop#build"]
    },
    "packager#package": {
      "dependsOn": ["ui#build", "desktop#bundle"],
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
